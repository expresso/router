{
  "name": "@expresso/router",
  "version": "2.0.4",
  "main": "dist/index.js",
  "description": "@expresso framework library",
  "keywords": [
    "swagger",
    "zod",
    "typescript",
    "openapi",
    "express",
    "expresso"
  ],
  "scripts": {
    "test": "mocha -b -r ts-node/register test/**/*.ts",
    "build": "tsc",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "build:clean": "npm run build && npm run build",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "dev": "tsnd usage",
    "prepack": "npm run build:clean",
    "push": "git push && sleep 2 && gh api repos/\\{owner\\}/\\{repo\\}/actions/runs?per_page=1 | jq '.workflow_runs[0].id' | xargs gh run watch"
  },
  "repository": "github:expresso/router",
  "author": "Roz <roz@rjmunhoz.me>",
  "license": "GPL-3.0",
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "chai": "^4.3.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^7.0.2",
    "mocha": "^9.1.0",
    "prettier": "^2.3.2",
    "semantic-release": "^17.4.7",
    "sinon": "^11.1.2",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@anatine/zod-openapi": "^0.4.0",
    "@expresso/app": "^1.2.4",
    "@expresso/server": "^1.1.4",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "openapi3-ts": "^2.0.1",
    "swagger-ui-express": "^4.1.6",
    "zod": "^3.8.1"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "docs",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ]
}
